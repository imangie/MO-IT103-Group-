<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="1024.0" prefWidth="1440.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.motorphui.HRLoginController">
   <children>
      <Label layoutX="488.0" layoutY="187.0" text="Welcome Back!">
         <font>
            <Font name="Poppins Bold" size="64.0" />
         </font>
      </Label>
      <PasswordField fx:id="email_field" layoutX="559.0" layoutY="358.0" prefHeight="44.0" prefWidth="365.0" promptText="Email">
         <font>
            <Font size="20.0" />
         </font>
      </PasswordField>
      <PasswordField fx:id="password_field" layoutX="559.0" layoutY="444.0" prefHeight="44.0" prefWidth="365.0" promptText="Password">
         <font>
            <Font name="Poppins Regular" size="20.0" />
         </font>
      </PasswordField>
      <Button fx:id="login_button" layoutX="559.0" layoutY="573.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="365.0" text="Login">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="trouble_label" layoutX="615.0" layoutY="693.0" text="Having trouble logging in?">
         <font>
            <Font name="Poppins Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="contact_label" layoutX="596.0" layoutY="725.0" text="Contact: corporate@motorph">
         <font>
            <Font name="Poppins Regular" size="18.0" />
         </font>
      </Label>
      <CheckBox fx:id="show_password_check" layoutX="560.0" layoutY="503.0" mnemonicParsing="false" text="Show Password">
         <font>
            <Font name="Poppins Regular" size="14.0" />
         </font>
      </CheckBox>
   </children>
</AnchorPane>
